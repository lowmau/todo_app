<div class="row">
	<div class="col-md-6">
  	<h1>Completados</h1>
  	  <table class="table table-striped">
			<tr>
		    <th>Description</th>
		    <th>Completed</br></th>
		  </tr>
  	<% @a.each do |a| %>
  	<tr>
		 <td><%= a.description %></td>
		 <td><%= a.completed %></td>
		 </tr>
		
	<% end %>
	</table>
  </div>
  
  <div class="col-md-6">
  	<h1>No completados</h1>
  	  <table class="table table-striped">
			<tr>
		    <th>Description</th>
		    <th>Completed</br></th>
		  </tr>
  	<% @b.each do |b| %>
  	<tr>
		 <td><%= b.description %></td>
		 <td><%= b.completed %></td>
		 </tr>
		
	<% end %>
	</table>
  </div>
</div>